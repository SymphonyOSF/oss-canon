{
  "name": "canon-runtime-ts",
  "private": true,
  "version": "1.0.0",
  "description": "Canon Typescript Runtime",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p ./ --outDir dist/",
    "watch": "tsc  -w -p ./ --outDir dist/ --preserveWatchOutput",
    "ut": "jest --verbose",
    "test": "yarn lint && yarn ut",
    "lint": "tslint -p ./ -t codeFrame"
  },
  "author": "Symphony",
  "engines": {
    "node": ">=14.6.0",
    "yarn": ">=1.15.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.6",
    "jest": "^22.4.3",
    "ts-jest": "22.4.4",
    "tslint": "^5.10.0",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@types/node": "^14.0.26"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/"
  }
}
